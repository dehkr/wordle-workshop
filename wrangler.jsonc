{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "wordle-workshop",
  "main": "src/index.ts",
  "compatibility_date": "2025-11-11",
  "compatibility_flags": [
    "global_fetch_strictly_public"
  ],
  "assets": {
    // The path to the directory containing the `index.html` file to be served at `/`
    "directory": "./public"
  },
  "observability": {
    "enabled": true
  }
}